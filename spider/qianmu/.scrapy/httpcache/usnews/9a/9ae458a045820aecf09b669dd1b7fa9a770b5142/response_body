	var needFill = '0';
 	var ads = '';  
var needpay = '0';
var vipnotice = '' ;
if(vl=='0'){
	
}
$(document).ready(function(){
	if(needlogin=='1'){
		location.href = "/loginView";
	}
	console.log(vipnotice);
	if(uId!=''&&vipnotice>0&&vipnotice<3){
		 swal({   
	           title: "您的VIP即将到期",   
