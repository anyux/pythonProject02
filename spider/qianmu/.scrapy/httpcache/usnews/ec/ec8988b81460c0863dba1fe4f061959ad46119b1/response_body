	           showCancelButton: true,   
	           confirmButtonColor: "#DD6B55",   
	           confirmButtonText: "购买VIP", 
	           cancelButtonText:'我知道了',
	           closeOnConfirm: true 
	       }, function(){   
	           location.href=ctx+"/zone/vippay2?vipRange=12";
	       });
	}
	  
